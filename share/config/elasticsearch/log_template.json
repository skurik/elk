{
  "template": "log-*",
  "settings": {
    "number_of_replicas": "0",
    "refresh_interval": "60s"
  },  
  "mappings": {
    "iis": {
      "properties": {
        "project_group_id": {
          "type": "long"
        },
        "project_id": {
          "type": "long"
        },       
        "segment_id": {
          "type": "long"
        },
        "time_taken" : {
          "type" : "long"
        },
        "status": {
          "type": "long"  
        },
        "sub_status": {
          "type": "long"
        },
        "win32_status": {
          "type": "long"
        },
        "bytes_sent": {
          "type": "long"
        },
        "bytes_received": {
          "type": "long"
        },
        "port": {
          "type": "long"
        },
        "geoip.location": {
          "type": "geo_point"
        },
        "request": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    },
    "iqube": {
      "properties": {
        "action": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "controller": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "log_level": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "action_phase": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "status_text": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "body": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "filename": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "log": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "assignment_prefixed_id": {
          "type": "string",
          "index": "not_analyzed" 
        },
        "project_group_id": {
          "type": "long"
        },
        "project_id": {
          "type": "long"
        },       
        "segment_id": {
          "type": "long"
        },
        "assignment_id": {
          "type": "long"
        },
        "status": {
          "type": "long"
        }
      }
    }
  }
}